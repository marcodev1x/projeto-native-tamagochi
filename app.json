      "permissions": [
        "BACKGROUND_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE"
      ],
      "package": "com.marcodev1x.TamagotchiApp"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a6020d55-6465-4558-b46a-3cdda4de851a"
      }
    }
  }
}
